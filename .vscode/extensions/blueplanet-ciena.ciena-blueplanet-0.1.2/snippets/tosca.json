{
  "Definition Module": {
    "prefix": [
      "dm",
      "definition-module"
    ],
    "body": [
      "\"\\$schema\"   = \"http://cyaninc.com/json-schemas/tosca-lite-v1/definition-module#\"",
      "title       = \"${1:DefinitionTitle}\"",
      "package     = ${2:exampe}",
      "version     = \"1.0\"",
      "description = \"\"\"",
      "${3:This TOSCA document defines...}",
      "\"\"\"",
      "",
      "authors     = [",
      "\t\"${4:Developer (developer@blueplanet.com)}\"",
      "]",
      "",
      "${5|resourceTypes,serviceTemplates,propertyTypes,capabilityTypes,relationshipTypes|} {",
      "\t${0}",
      "}"
    ],
    "description": "TOSCA Definition Module"
  },
  "Imports Block": {
    "prefix": [
      "imports",
      "imb"
    ],
    "body": [
      "imports {",
      "\t${1:Alias} = ${2:package}.${3|resourceTypes,serviceTemplates,propertyTypes,capabilityTypes,relationshipTypes|}.${4:ArtifactName}",
      "}"
    ],
    "description": "TOSCA Imports Block & Import"
  },
