  "Resource Type Definition": {
    "prefix": [
      "rt",
      "resourceType"
    ],
    "body": [
      "${1:ResourceType} {",
      "\ttitle       = ${2:Resource Type}",
      "\tdescription = ${3:Resource type example}",
      "\tderivedFrom = ${4:tosca.resourceTypes.Root}",
      "\tproperties {",
      "\t\t${0}",
      "\t}",
      "}"
    ],
    "description": "TOSCA Resource Type Defintion"
  },
  "Service Template Definition": {
    "prefix": [
      "st",
      "serviceTemplate"
    ],
    "body": [
      "${1:Service} {",
      "\ttitle       = ${2:Service Title}",
      "\tdescription = ${3:Service example}",
      "\timplements  = ${4:package}.resourceTypes.${5:ResourceType}",
      "\t${6|resources,plans|} {",
      "\t\t${0}",
      "\t}",
      "}"
    ],
    "description": "TOSCA Service Template Definition"
  },
  "Capability Definition": {
    "prefix": [
      "cd",
      "capability"
    ],
    "body": [
      "${1:Capability} {",
      "\tderivedFrom = ${2:tosca}.capabilityTypes.${3:CapabilityType}",
      "\ttitle       = ${4:Capability Title}",
      "\tdescription = ${5:Capability description}",
      "}${0}"
    ]
  },
  "Relationship Definition": {
    "prefix": [
      "rd",
      "Relationship"
    ],
    "body": [
      "${1:Relationship} {",
      "\tderivedFrom = ${2:tosca}.relationshipTypes.${3:RelationshipType}",
      "\ttitle       = ${4:Relationship Title}",
      "\tdescription = ${5:Relationship description}",
      "}${0}"
    ]
  },
  "Property Definition": {
    "prefix": [
      "pr",
      "prop"
    ],
    "body": [
      "${1:property} {",
      "\ttitle       = ${2:Property}",
      "\tdescription = ${3:Property Description}",
      "\ttype        = ${4|array,boolean,integer,null,number,object,string|}",
      "}${0}"
    ],
    "description": "TOSCA Property"
  },
