  "Interfaces Attribute": {
    "prefix": [
      "interfaces"
    ],
    "body": [
      "interfaces {",
      "\t${0}",
      "}"
    ],
    "description": "TOSCA Resource Type Interfaces Block"
  },
  "Interfaces": {
    "prefix": [
      "interface"
    ],
    "body": [
      "${1:MyInterface} {",
      "\ttitle = ${2:MyInterface}",
      "\tdescription = ${3:My Interface Description}",
      "\tinputs {",
      "\t\t${0}",
      "\t}",
      "\toutputs {",
      "\t\t",
